{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/admin.html",
      "destination": "/frontend/admin.html"
    }
  ]
}
